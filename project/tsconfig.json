{
    "compilerOptions": {
        "allowJs": true,
        "target": "ES5",
        "outDir": "./built",
        "moduleResolution": "node",
        "lib":["ES2015", "DOM", "DOM.Iterable"],
        "noImplicitAny": true,
        "strict": true,
        "strictNullChecks": true,
        "strictFunctionTypes": true,
        "strictBindCallApply": true,
        "strictPropertyInitialization": true,
        "alwaysStrict": true,
    },
    "include": ["./src//**/*"]
}